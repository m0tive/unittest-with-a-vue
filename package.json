{
  "name": "unittest-with-a-vue",
  "version": "1.0.0",
  "description": "Kickabout with vue.js",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "vue": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.3",
    "node-sass": "^4.5.0",
    "sass-loader": "^5.0.1",
    "testcafe": "^0.13.0",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "scripts": {
    "test": "karma start test/unit/karma.conf.js --single-run && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "e2e": "testcafe chrome,firefox test/e2e/test.js --app \"npm run dev-e2e\"",
    "travis": "./node_modules/.bin/karma start test/unit/karma.conf.js --browsers Firefox --single-run",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "dev-e2e": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m0tive/unittest-with-a-vue.git"
  },
  "author": "Peter Dodds",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m0tive/unittest-with-a-vue/issues"
  },
  "homepage": "https://github.com/m0tive/unittest-with-a-vue#readme"
}
